  pod 'FacebookCore', '~> 0.3'
  pod 'FacebookLogin', '~> 0.3'
  pod 'FacebookShare', '~> 0.3'

  pod 'Kingfisher', '~> 4.1'
  pod 'R.swift', '~> 4.0'
end

target 'GetMoreGamesTests' do
  use_frameworks!

  pod 'Quick', '~> 1.2'
end
